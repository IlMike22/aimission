        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainScene.Views.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AimListScene.Views.AimListActivity" />
        <activity android:name=".MonthListScene.Views.MonthListActivity" />
        <activity android:name=".InfoScene.Views.InfoActivity" />
        <activity android:name=".SettingsScene.Views.SettingsActivity" />
        <activity android:name=".Base.BaseActivity" />
        <activity android:name=".RegisterScene.Views.RegisterActivity"></activity>
    </application>

</manifest>